{
  "name": "@openmrs/esm-cohort-builder-app",
  "version": "3.0.0",
  "license": "MPL-2.0",
  "description": "A microfrontend for OpenMRS cohorts",
  "browser": "dist/openmrs-esm-cohort-builder-app.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "start": "openmrs develop",
    "serve": "webpack serve --mode=development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typescript": "tsc",
    "test": "jest --config jest.config.json --verbose",
    "verify": "concurrently 'yarn:lint' 'yarn:test' 'yarn:typescript'",
    "coverage": "yarn test -- --coverage",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn verify"
    }
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs",
    "microfrontends"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-cohortbuilder.git"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-cohortbuilder#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-cohortbuilder/issues"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "swr": "^1.3.0"
  },
  "peerDependencies": {
    "@carbon/react": "^1.9.0",
    "@openmrs/esm-framework": "4.x",
    "@openmrs/esm-patient-common-lib": "^4.0",
    "dayjs": "^1.11.3",
    "react": "18.x",
    "react-dom": "18.x",
    "react-i18next": "11.x",
    "react-router-dom": "6.x"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@carbon/react": "^1.9.0",
    "@openmrs/esm-framework": "4.0.1-pre.232",
    "@openmrs/esm-patient-common-lib": "^4.0.1-pre.235",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.7",
    "@types/react-dom": "^16.9.14",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^6.2.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "openmrs": "4.0.1-pre.232",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.4",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.3.2"
  }
}
